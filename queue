#include<iostream>
#include<conio.h>

using namespace std;

int main(){
    int a[5],c=0,t,back=-1;
    
while(c!=4){
    
    cout<<"\nEnter your choice: ";
    cout<<"\n1.Enqueue";
    cout<<"\n2.Dequeue";
    cout<<"\n3.Look";
    cout<<"\n4.Exit\n";
    cin>>c;
    
    switch(c){
        
        case 1: if(back==4)
                cout<<"Queue if full!\n";
                else{
                    cout<<"Enter and element: ";
                    cin>>t;
                    a[++back]=t;}
                    break;
        case 2: if(back==-1)
                cout<<"Queue if empty!\n";
                else{
                cout<<"Removed element is "<<a[0];
                --back;}
                break;
        case 3: if(back==-1)
                cout<<"Queue if empty!\n";
                else
                cout<<"Element at last is "<<a[back]<<" and element at front is "<<a[0];
                break;
        case 4: cout<<"Exiting program...";
}
}
}
